<Page x:Class="Sucrose.Portal.Views.Pages.SettingPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Sucrose.Portal.Views.Pages"
      xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
      Title="SettingPage"
      mc:Ignorable="d">
    <Grid x:Name="GridSetting" Loaded="GridSetting_Loaded">
        <Frame x:Name="FrameSetting" Visibility="Collapsed" />
        <ui:ProgressRing x:Name="ProgressSetting" IsIndeterminate="True" Width="100" Height="100" />
    </Grid>
</Page>